{
  "name": "rv2me",
  "version": "6.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start-docker": "docker-compose up -d express",
    "eslint": "eslint .; exit 0",
    "lesswatch": "less-watch-compiler public/stylesheets/modules public/stylesheets style.less",
    "test-6": "docker-compose run node_6_alpine npm test",
    "test-7": "docker-compose run node_7_alpine npm test",
    "test-latest": "docker-compose run node_latest npm test",
    "test": "nyc --reporter=html --reporter=text mocha test/**/*.js",
    "test-scrutinizer": "nyc --all --reporter=html --reporter=clover mocha test/**/*.js",
    "report-coveralls": "nyc report --reporter=text-lcov | coveralls",
    "report-codecov": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.9",
    "express": "^4.15.5",
    "jstransformer-markdown-it": "^2.0.0",
    "mongodb": "^2.2.33",
    "morgan": "^1.9.0",
    "pug": "^2.0.0-beta11",
    "serve-favicon": "^2.4.5",
    "ws": "^3.3.2"
  },
  "main": "app.js",
  "author": "Magnus Andersson",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "clean-css-cli": "^4.1.10",
    "codecov": "^3.0.0",
    "coveralls": "^3.0.0",
    "csslint": "^1.0.5",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "html-minifier": "^3.5.6",
    "htmlhint": "^0.9.13",
    "js-yaml": "^3.10.0",
    "jscs": "^3.0.7",
    "jsonlint": "^1.6.2",
    "lesshint": "^4.4.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "stylelint": "^8.2.0",
    "uglify-js": "^3.1.8"
  }
}
